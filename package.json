{
  "name": "mydevelopper",
  "displayName": "mydevelopper",
  "description": "use autogpt like functions in vscode",
  "version": "0.0.2",
  "repository": "git@github.com:OdranHUSSON/mydevelopper.git",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mydevelopper.addCode"
  ],  
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mydevelopper.addCode",
        "title": "ðŸ¤– Add Code"
      },
      {
        "command": "mydevelopper.commit",
        "title": "ðŸ¤– Generate a commit message and commit"
      }
    ],
    "configuration": {
      "title": "mydevelopper",
      "properties": {
        "mydevelopper.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "Your OpenAI API Key"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "openai": "^3.2.1"
  }
}
